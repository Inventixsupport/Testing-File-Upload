generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserInfo {
  id        Int      @id @default(autoincrement())
  username  String
  image     String?  // filename stored here (e.g. "john-1234.jpg")
  createdAt DateTime @default(now())
}
